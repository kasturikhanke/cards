{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/dist/(.*)",
        "dest": "/dist/$1"
      },
      {
        "src": "/(.*)\\.js",
        "dest": "/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }